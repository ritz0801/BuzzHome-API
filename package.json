{
  "name": "buzzhome-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "nodemon ./src/bin/www --exec babel-node --presets-env",
    "start": "nodemon ./src/bin/www --exec babel-node --presets-env",
    "server": "babel-node ./src/bin/www",
    "build": "babel -d ./build/src ./src && babel -d ./build/database/models ./database/models",
    "prod": "npm run build && node build/src/bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "http-status-codes": "^2.1.3",
    "morgan": "~1.9.1",
    "node-geocoder": "^3.27.0",
    "nodemailer": "^6.6.3",
    "pg": "^8.3.2",
    "sequelize": "^6.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0"
  }
}
